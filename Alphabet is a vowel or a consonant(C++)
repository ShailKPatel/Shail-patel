#include<iostream>
using namespace std;
int main(){

    char a;
    cout<<"Enter a alphabet : ";
    cin>>a;

    switch(a){
        case 'a':
        case 'A':
        case 'e':
        case 'E':
        case 'i':
        case 'I':
        case 'o':
        case 'O':
        case 'u':
        case 'U':
            cout<<"Vowel";
            break;
        default:
             cout<<"Consonant";
             break;
    }

    return 0;
}
