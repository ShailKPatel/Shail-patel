#include<iostream>
using namespace std;
int main(){
    const char* n=" is larger";
    
    int x,y,z;

    cout<<"Enter three number:"<<endl;


    cin>>x>>y>>z;
    if(x==y && y==z){
        cout<<"They are equal"<<endl;
    }
    else if(x>y){
        if(x>z){
        cout<<x<<n<<endl;
        }
        else{
            cout<<z<<n<<endl;
        }
    }
    else{
        if(y>z){
        cout<<y<<n<<endl;
        }
        else{
            cout<<z<<n<<endl;
        }
    }
    return 0;
}
