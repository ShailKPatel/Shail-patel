#include<iostream>
using namespace std;
int main(){
    int n,i;
    cout<<"Enter a number :";
    cin>>n;
    if(n==1){
            
            cout<<"Its neither prime nor composite";
        }
    for(i=2;i<n;i++){
        if(n%i==0){
            cout<<"composite";
            break;
        }
        else{
            cout<<"Prime";
            break;
        }
    }

    return 0;
}
